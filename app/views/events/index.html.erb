<h1 class="text-center">Homepage</h1>
<h2 class="text-center m-4">Upcoming Events</h2>
<% unless @events.empty? %>
    <div class="container d-flex flex-wrap justify-content-center">
        <% @events.each do |event| %>
            <%= render "event", event: event %>
        <% end %>
    </div>
<% end %>